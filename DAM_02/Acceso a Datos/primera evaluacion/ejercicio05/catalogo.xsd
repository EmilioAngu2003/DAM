    <xs:import namespace="http://www.ejemplo.com/peliculas" schemaLocation="pelicula.xsd"/>

    <xs:element name="catalogo">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="pelicula" type="p:peliculaType" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

</xs:schema>
